{
  "name": "@maxjustus/chttp",
  "version": "1.14.0",
  "description": "ClickHouse HTTP/TCP client with native compression (LZ4/ZSTD) and Native format support",
  "type": "module",
  "main": "./dist/chttp.js",
  "bin": {
    "chttp": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/chttp.js",
      "require": "./dist/chttp.cjs"
    },
    "./lz4": {
      "import": "./dist/chttp-lz4.js",
      "require": "./dist/chttp-lz4.cjs"
    },
    "./native": {
      "development": "./native/index.ts",
      "import": "./dist/native.js",
      "require": "./dist/native.cjs"
    },
    "./source": "./index.ts",
    "./tcp": {
      "development": "./tcp_client/index.ts",
      "import": "./dist/tcp.js",
      "require": "./dist/tcp.cjs"
    }
  },
  "files": [
    "dist",
    "vendor",
    "*.ts",
    "*.d.ts",
    "tcp_client",
    "native"
  ],
  "scripts": {
    "prepare": "cp .hooks/* .git/hooks/ 2>/dev/null || true",
    "prepublishOnly": "npm run build",
    "build": "npm run build:native && npm run build:native:cjs && npm run build:full && npm run build:full:cjs && npm run build:lz4 && npm run build:lz4:cjs && npm run build:tcp && npm run build:tcp:cjs && npm run build:cli && npm run build:copy-wasm",
    "build:cli": "esbuild tcp_client/cli.ts --bundle --format=esm --outfile=dist/cli.js --platform=node --target=es2022 --external:lz4-napi --external:zstd-napi",
    "build:copy-wasm": "cp node_modules/@bokuweb/zstd-wasm/dist/common/zstd.wasm dist/",
    "build:native": "esbuild native/index.ts --bundle --format=esm --outfile=dist/native.js --platform=neutral --target=es2022 --minify --external:lz4-napi --external:zstd-napi",
    "build:native:cjs": "esbuild native/index.ts --bundle --format=cjs --outfile=dist/native.cjs --platform=node --target=es2022 --minify --external:lz4-napi --external:zstd-napi",
    "build:full": "esbuild index.ts --bundle --format=esm --outfile=dist/chttp.js --platform=browser --target=es2022 --minify --define:BUILD_WITH_ZSTD=true --external:zstd-napi --external:lz4-napi --external:@maxjustus/chttp/native --alias:@maxjustus/chttp/native=@maxjustus/chttp/native",
    "build:full:cjs": "esbuild index.ts --bundle --format=cjs --outfile=dist/chttp.cjs --platform=node --target=es2022 --minify --define:BUILD_WITH_ZSTD=true --external:zstd-napi --external:lz4-napi --external:@maxjustus/chttp/native --alias:@maxjustus/chttp/native=@maxjustus/chttp/native",
    "build:lz4": "esbuild index.ts --bundle --format=esm --outfile=dist/chttp-lz4.js --platform=browser --target=es2022 --minify --define:BUILD_WITH_ZSTD=false --external:zstd-napi --external:lz4-napi --external:@maxjustus/chttp/native --alias:@maxjustus/chttp/native=@maxjustus/chttp/native",
    "build:lz4:cjs": "esbuild index.ts --bundle --format=cjs --outfile=dist/chttp-lz4.cjs --platform=node --target=es2022 --minify --define:BUILD_WITH_ZSTD=false --external:zstd-napi --external:lz4-napi --external:@maxjustus/chttp/native --alias:@maxjustus/chttp/native=@maxjustus/chttp/native",
    "build:tcp": "esbuild tcp_client/index.ts --bundle --format=esm --outfile=dist/tcp.js --platform=node --target=es2022 --minify --external:lz4-napi --external:zstd-napi --external:@maxjustus/chttp/native --alias:@maxjustus/chttp/native=@maxjustus/chttp/native",
    "build:tcp:cjs": "esbuild tcp_client/index.ts --bundle --format=cjs --outfile=dist/tcp.cjs --platform=node --target=es2022 --minify --external:lz4-napi --external:zstd-napi --external:@maxjustus/chttp/native --alias:@maxjustus/chttp/native=@maxjustus/chttp/native",
    "test": "tsc --noEmit && NODE_OPTIONS=--conditions=development tsx --test test/native/*.test.ts test/*.test.ts",
    "test:compression": "NODE_OPTIONS=--conditions=development tsx --test test/compression.test.ts",
    "test:integration": "NODE_OPTIONS=--conditions=development tsx --test test/integration.test.ts",
    "test:native": "NODE_OPTIONS=--conditions=development tsx --test test/native/*.test.ts",
    "test:dist": "npm run build && node test/test-dist.mjs",
    "test:fuzz": "node --experimental-strip-types fuzz/parallel.ts --all",
    "test:fuzz:quick": "node --experimental-strip-types fuzz/parallel.ts --all --level=quick",
    "test:fuzz:verbose": "node --experimental-strip-types fuzz/parallel.ts --all --verbose",
    "test:tcp": "NODE_OPTIONS=--conditions=development tsx --test tcp_client/test/*.test.ts",
    "bench": "tsx bench/compression.ts",
    "bench:insert": "tsx bench/insert.ts",
    "vendor:lz4": "npx jsr add @nick/lz4 && cp node_modules/@nick/lz4/lib/lz4.js node_modules/@nick/lz4/lib/lz4.internal.js vendor/lz4/ && rm -rf node_modules/@nick",
    "format": "biome format --write .",
    "format:check": "biome check ."
  },
  "author": "maxjustus",
  "license": "MIT",
  "dependencies": {
    "@bokuweb/zstd-wasm": "^0.0.27",
    "ch-city-wasm": "^0.1.1"
  },
  "optionalDependencies": {
    "lz4-napi": "^2.9.0",
    "zstd-napi": "^0.0.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@testcontainers/clickhouse": "^11.5.1",
    "esbuild": "^0.27.0",
    "testcontainers": "^11.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
