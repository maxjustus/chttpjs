{
  "name": "@maxjustus/chttp",
  "version": "1.3.3",
  "description": "ClickHouse HTTP client with native compression (LZ4/ZSTD)",
  "type": "module",
  "main": "./dist/chttp.js",
  "exports": {
    ".": "./dist/chttp.js",
    "./lz4": "./dist/chttp-lz4.js",
    "./source": "./index.ts"
  },
  "files": [
    "dist",
    "vendor",
    "*.ts",
    "*.d.ts"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run build:full && npm run build:lz4",
    "build:full": "esbuild index.ts --bundle --format=esm --outfile=dist/chttp.js --platform=browser --target=es2022 --minify --define:BUILD_WITH_ZSTD=true --external:zstd-napi --external:lz4-napi",
    "build:lz4": "esbuild index.ts --bundle --format=esm --outfile=dist/chttp-lz4.js --platform=browser --target=es2022 --minify --define:BUILD_WITH_ZSTD=false --external:zstd-napi --external:lz4-napi",
    "test": "tsc --noEmit && node --experimental-strip-types --test test/*.test.ts",
    "test:compression": "node --experimental-strip-types --test test/compression.test.ts",
    "test:integration": "node --experimental-strip-types --test test/integration.test.ts",
    "test:dist": "npm run build && node test/test-dist.mjs",
    "test:fuzz": "node --experimental-strip-types --test test/rowbinary-fuzz.test.ts test/native-fuzz.test.ts",
    "bench": "node --experimental-strip-types bench/compression.ts",
    "bench:insert": "node --experimental-strip-types bench/insert.ts",
    "vendor:lz4": "npx jsr add @nick/lz4 && cp node_modules/@nick/lz4/lib/lz4.js node_modules/@nick/lz4/lib/lz4.internal.js vendor/lz4/ && rm -rf node_modules/@nick"
  },
  "author": "maxjustus",
  "license": "ISC",
  "dependencies": {
    "@bokuweb/zstd-wasm": "^0.0.27",
    "ch-city-wasm": "^0.1.1"
  },
  "optionalDependencies": {
    "lz4-napi": "^2.9.0",
    "zstd-napi": "^0.0.12"
  },
  "devDependencies": {
    "@clickhouse/client": "^1.14.0",
    "@dweb-browser/zstd-wasm": "^1.2.1",
    "@hpcc-js/wasm-zstd": "^1.8.0",
    "@lichtblick/wasm-zstd": "^1.0.0",
    "@mongodb-js/zstd": "^7.0.0",
    "@oneidentity/zstd-js": "^1.0.3",
    "@testcontainers/clickhouse": "^11.5.1",
    "esbuild": "^0.27.0",
    "fast-json-stringify": "^6.1.1",
    "flatstr": "^1.0.12",
    "testcontainers": "^11.5.1",
    "typescript": "^5.9.3",
    "zstd-wasm-decoder": "^0.2.2"
  }
}
